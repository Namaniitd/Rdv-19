aws dynamodb create-table  --table-name 2018_RDV_Registrations --attribute-definitions AttributeName=rdv_number,AttributeType=S AttributeName=email,AttributeType=S --key-schema AttributeName=rdv_number,KeyType=HASH  --provisioned-throughput ReadCapacityUnits=1,WriteCapacityUnits=1              --global-secondary-indexes IndexName="email",\ KeySchema="{AttributeName=email,KeyType=HASH}",\ Projection="{ProjectionType=ALL}",\ ProvisionedThroughput="{ReadCapacityUnits=10,WriteCapacityUnits=10}" --endpoint-url http://localhost:8000