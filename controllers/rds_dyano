REgTable
aws dynamodb create-table     --table-name 2018_RDV_Registrations     --attribute-definitions         AttributeName=email,AttributeType=S         AttributeName=rdv_number,AttributeType=S     --key-schema AttributeName=email,KeyType=HASH AttributeName=rdv_number,KeyType=RANGE --provisioned-throughput ReadCapacityUnits=1,WriteCapacityUnits=1   --global-secondary-indexes 'IndexName=email,  KeySchema=[{AttributeName=email,KeyType=HASH},{AttributeName=rdv_number,KeyType=RANGE}], Projection={ProjectionType=ALL}, ProvisionedThroughput={ReadCapacityUnits=10,WriteCapacityUnits=10}' --endpoint-url http://localhost:8000

team table
aws dynamodb create-table     --table-name 2018_RDV_Team     --attribute-definitions         AttributeName=email,AttributeType=S     --key-schema AttributeName=email,KeyType=HASH --provisioned-throughput ReadCapacityUnits=1,WriteCapacityUnits=1   --endpoint-url http://localhost:8000

rdv_resources
aws dynamodb create-table     --table-name RDV_Resources     --attribute-definitions         AttributeName=type,AttributeType=S AttributeName=key,AttributeType=S     --key-schema AttributeName=type,KeyType=HASH AttributeName=key,KeyType=RANGE --provisioned-throughput ReadCapacityUnits=1,WriteCapacityUnits=1   --endpoint-url http://localhost:8000