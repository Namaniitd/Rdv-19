<script>
$('.slide').scroll(function() {
	var img = $('.img-parallax2');
	var topOffset = img.offset().top;
	var y = topOffset/3;
    var t=window.matchMedia("(max-width:768px)");
    if(t.matches)
    var prop =  "translate3d(0,"+(y-50)+"px,0) rotateZ(-20deg)";
    else
	var prop = "translate3d(0,"+(y+100)+"px,0) rotateZ(-20deg)";
	img.css("transform",prop);
});
$('.slide').scroll(function() {
    var img = $('.img-parallax3');
    var topOffset = img.offset().top;
    var y = topOffset/6;
    var t=window.matchMedia("(max-width:768px)");
    if(t.matches)
    var prop =  "translate3d(-30px,"+(y+180)+"px,0) rotateZ(0deg)";
    else
    var prop = "translate3d(-30px,"+(y+240)+"px,0) rotateZ(0deg)";
    img.css("transform",prop);
});
$('.slide').scroll(function() {
    var img = $('.img-parallax4');
    var topOffset = img.offset().top;
    var y = topOffset/4;
    var t=window.matchMedia("(max-width:768px)");
    if(t.matches)
    var prop =  "translate3d(50px,"+(y+90)+"px,0) rotateZ(70deg)";
    else
    var prop = "translate3d(-30px,"+(y+190)+"px,0) rotateZ(70deg)";
    img.css("transform",prop);
});
