<script>
function looper(url, interval){
    // If interval is undefined, we set the value to 2000 (initial)
    // Otherwise, we use the value passed in
    interval = interval === undefined ? 2000 : interval;
    if (url) {
        document.getElementById('gramo').src = url;
    } else {
        url = "../../assets/images/elements/gramo1.png";
    }
    // Timer logic to handle the next iteration
    setTimeout(function() {
        if (url === "https://assets.rdviitd.org/images/elements/gramo.png") {
            looper("https://assets.rdviitd.org/images/elements/gramo1.png", 300);
        } else {
            looper("https://assets.rdviitd.org/images/elements/gramo.png", 300);
        }
    }, interval);
}
// looper('../../assets/images/elements/gramo.png',200);
</script>
